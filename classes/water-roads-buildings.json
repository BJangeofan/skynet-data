[{
  "name": "Water",
  "color": "#0000ff",
  "stroke-width": "5",
  "filter": "[waterway].match('.+') or [natural] = 'water' or [natural] = 'bay'"
}, {
  "name": "Road",
  "color": "#ffffff",
  "stroke-width": "5",
  "filter": "[highway].match('.+') and not ([tunnel] = 'yes' or [tunnel]='true')"
}, {
  "name": "Building",
  "color": "#ff0000",
  "stroke-width": "1",
  "filter": "[building].match('.+')"
}]
